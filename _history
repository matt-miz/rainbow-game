ction * speed\n        if bouncer >= 150:\n            direction = -1\n        elif bouncer <= 0:\n            direction = 1\n        if bouncer <= 10 and old_bouncer >= 10 and direction == -1:\n            list2 = [0]\n            led.toggle(0, 0)\n        elif bouncer >= 10 and old_bouncer <= 10 and direction == 1:\n            led.toggle(0, 0)\n            max_acc2 = 0\n            for i in list2:\n                max_acc2 = max(max_acc2, i)\n            if max_acc2 > 2.5:\n                score = score + 1\n            else:\n                score = max(score - 1, 0)\n                led.toggle(4, 4)\n            serial.write_value(\"score\", score)\n        elif bouncer <= 10:\n            list2.append(acc)\n        strip_bouncer.show_bar_graph(bouncer, 150)\nbasic.forever(on_forever4)\n","pxt.json":"{\n    \"name\": \"Rainbow-game\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\",\n        \"neopixel\": \"github:microsoft/pxt-neopixel#v0.7.6\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\",\n        \"main.py\"\n    ],\n    \"preferredEditor\": \"pyprj\"\n}\n"}},{"timestamp":1731879682858,"editorVersion":"7.0.51","text":{"main.blocks":"<xml xmlns=\"https://developers.google.com/blockly/xml\"><variables><variable id=\"sp%9fEa=$d_@M/.p4a[X\">started</variable><variable id=\"+0sm^HCbb-!:-aCJ{5bH\">winning_score</variable><variable id=\"(plGp1xM`%dX8Y/azT1z\">debug</variable><variable id=\"2caOO~^[TRDv+pMEb@F0\">acc</variable><variable id=\"6BcwDv|eDCN{D!b(Zq(W\">score</variable><variable id=\"c`5YlMW}aV2yU0y^,|4F\">running</variable><variable id=\"M8FOlzY5UdTVBgl=QT3*\">strip_bouncer</variable><variable id=\"_$Y[W#@T)/9|6ErAv]rn\">strip_score</variable><variable id=\"DcDUxa7w#NQAX0-qgsA|\">speed</variable><variable id=\"(61=Y)m|3ese;`:4(7-^\">old_bouncer</variable><variable id=\"y5+J}wFBC*gd55UoFUY]\">bouncer</variable><varia